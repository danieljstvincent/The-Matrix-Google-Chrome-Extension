{
  "manifest_version": 3,
  "name": "Welcome - New Tab",
  "description": "Get welcomed with a beautiful photo and the time on each new tab.",
  "version": "1.0.0",
  "icons": {
    "16": "images/light_bulb_mascot_16x16.png",
    "32": "images/light_bulb_mascot_38x38.png",
    "48": "light_bulb_mascot_48x48.png",
    "128": "images/light_bulb_mascot_128x128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/light_bulb_mascot_16x16.png",
      "32": "images/light_bulb_mascot_38x83",
      "48": "images/light_bulb_mascot_48x48.png",
      "128": "images/light_bulb_mascot_128x128.png"
    },
    "default_title": "Open New Tab"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
