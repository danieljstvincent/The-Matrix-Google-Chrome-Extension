{
  "manifest_version": 3,
  "name": "Welcome - New Tab",
  "description": "Get welcomed with a beautiful photo and the time on each new tab.",
  "version": "1.0.0",
  "icons": {
    "128": "images/icon.png"
  },
  "action": {
    "default_icon": {
      "32": "images/icon.png"
    },
    "default_title": "Open New Tab"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
